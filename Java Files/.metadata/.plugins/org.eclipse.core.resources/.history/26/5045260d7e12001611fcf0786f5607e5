package qeustion_1;

public class Data {
	
	private int day;
	private int month;
	private int year;

public receiver(int a, int b, int c){

	day = a;
	month = b;
	year = c;


}
	
public getDay(){

	return day;

}	


public getMonth(){

	return month;

}	


public getYear(){

	return year;

}

public void printData(){

System.out.printf("Data: %d/%d/%d\n", getDay(),getMonth(),getYear());			

}





}
