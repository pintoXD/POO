	//public void 

	public data(int a, int b, int c) {
		// TODO Auto-generated constructor stub
	
	day = a;
	month = b;
	year = c;
	
	}


	public int getDay(){

		return day;

	}	


	public int getMonth(){

		return month;

	}	


	public int getYear(){

		return year;

	}

	public void printData(){

		System.out.printf("Data: %d/%d/%d\n", getDay(),getMonth(),getYear());			

	}





}
